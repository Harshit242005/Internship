@page
@model Enhance.Pages.CreategroupModel
@{
    ViewData["Title"] = "Create Group page";
}

<head>
    <link rel="stylesheet" href="~/css/Creategroup.css?version=1" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Readex+Pro:wght@200;300;400;500&display=swap" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<form method="post">
    <div class="Name">
        <input type="text" placeholder="Enter group name" autocomplete="off" asp-for="Name"/>
    </div>
    <button type="submit">Create</button>
</form>

